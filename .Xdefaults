# Urxvt Settings

!URxvt.font: xft:inconsolata:pixelsize=12
!URxvt.font: xft:lime
!URxvt.font: xft:Terminus-8
!URxvt.font: xft:Liberation Mono-8
!URxvt.font: xft:Bitstream Vera Sans Mono-8
!URxvt.font: xft:Envy Code R:pixelsize=11
URxvt.font: xft:Droid Sans Mono:pixelsize=10
!URxvt.font: xft:gohufont-8
!URxvt.font: xft:MonteCarlo-9
URxvt.scrollBar: false
URxvt.urgentOnBell: true
URxvt.perl-ext: default,matcher
URxvt.urlLauncher: /usr/bin/chromium
URxvt.matcher.button:  1 
URxvt.geometry: 110x30
!URxvt.visualBell: True

# Font hinting

Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.hintstyle: hintfull

